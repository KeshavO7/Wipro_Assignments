<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">
 
    <bean id="customer" class="com.model.Customer">
        <property name="sno" value="100"></property>
        <property name="name" value="Keshav"></property>
        <property name="email" value="keshav@gmail.com"></property>
    </bean>
    
     <bean id="address" class="com.model.Address">
        <property name="doorno" value="100"></property>
        <property name="streetAddr" value="Darbhanga"></property>
        <property name="cityName" value="Biahr"></property>
        <property name="pincode" value="1234"></property>
    </bean>
</beans>